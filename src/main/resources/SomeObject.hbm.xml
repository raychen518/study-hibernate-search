<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.raychen518.study.hibernatesearch">
    <class name="SomeObject" table="SOME_OBJECTS">
        <id name="id" column="ID">
            <generator class="increment" />
        </id>
        <property name="field1" column="FIELD_1" />
        <property name="field2" column="FIELD_2" />
        <property name="field3" column="FIELD_3" />
    </class>
</hibernate-mapping>
